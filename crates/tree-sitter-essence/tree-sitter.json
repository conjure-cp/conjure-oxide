{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "essence",
      "camelcase": "Essence",
      "title": "Essence",
      "scope": "source.essence",
      "file-types": [
        "essence",
        "eprime"
      ],
      "injection-regex": "^essence$",
      "class-name": "TreeSitterEssence"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MPL-2.0",
    "description": "Essence grammar for tree-sitter",
    "authors": [
      {
        "name": "Leia Young"
      }
    ],
    "links": {
      "repository": "https://github.com/conjure-cp/conjure-oxide"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
